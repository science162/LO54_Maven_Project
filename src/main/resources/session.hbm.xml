<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="fr.utbm.core.entity.Session" table="session"> 
	  <id column="id_session" name="id_session">
		<generator class="identity"/>
	  </id>
	  <property column="start_date" name="start_date" not-null="true"/>
	  <property column="end_date" name="end_date" not-null="true"/>
	  <property column="nbre_place" name="nbre_place" not-null="true"/>
	  <many-to-one column="id_course" name="course" class="fr.utbm.core.entity.Course"/>
	  <many-to-one column="id_location" name="location" class="fr.utbm.core.entity.Location" unique="true"/>	  
  </class>
  
</hibernate-mapping>
											